{
    "name": "wiperawa/yii2-gallery",
    "description": "Модуль позволяет загружать виджетом картинки",
    "keywords": ["yii2", "yii2", "application", "images", "yii2-images", "wiperawa"],
    "homepage": "https://github.com/wiperawa/yii2-gallery",
    "type": "yii2-extension",
    "license": "Apache-2.0",
    "support": {
        "source": "https://github.com/wiperawa/yii2-gallery"
    },
    "authors": [
        {
            "name": "Dmitriy Lukeryanov",
            "email": "wiperawa@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "kartik-v/yii2-widget-fileinput": "*",
        "abeautifulsite/simpleimage": "2.*",
        "bower-asset/pica": "5.*"
    },
    "config": {
        "process-timeout": 1800
    },
    "autoload": {
        "psr-4": {
            "wiperawa\\gallery\\": "src\\"
        }
    }
}
